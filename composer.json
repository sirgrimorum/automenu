{
    "name": "sirgrimorum/automenu",
    "type": "library",
    "description": "Smart menu form Laravel 5.6",
    "keywords": [
        "sirgrimorum",
        "AutoMenu"
    ],
    "homepage": "https://github.com/sirgrimorum/automenu",
    "license": "MIT",
    "authors": [
        {
            "name": "SirGrimorum",
            "email": "andres.espinosa@grimorum.com",
            "homepage": "https://github.com/sirgrimorum",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Sirgrimorum\\AutoMenu\\": "src/"
        }
    },
    "autoload-dev": {
        
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2.5-dev"
        },
        "laravel": {
            "providers": [
                "Sirgrimorum\\AutoMenu\\AutoMenuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
